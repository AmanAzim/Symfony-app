<div class="flex">
    {% embed 'shared/_tabs.html.twig' %}
        {% block tabs %}
            {% embed 'shared/_tab.html.twig' with { route: 'app_micro_post' } %}
                {% block content %}
                    All posts <span class="px-2 rounded-md bg-gray-200 dark:bg-gray-700" ></span>
                {% endblock %}
                {% block path %}
                    {{ path('app_micro_post') }}
                {% endblock %}
            {% endembed %}
        {% endblock %}
    {% endembed %}

    {% embed 'shared/_tabs.html.twig' %}
        {% block tabs %}
            {% embed 'shared/_tab.html.twig' with { route: 'app_micro_post_topLiked', classes: 'ml-2' } %}
                {% block content %}
                   Top likes posts <span class="px-2 rounded-md bg-gray-200 dark:bg-gray-700" ></span>
                {% endblock %}
                {% block path %}
                    {{ path('app_micro_post_topLiked') }}
                {% endblock %}
            {% endembed %}
        {% endblock %}
    {% endembed %}

    {% embed 'shared/_tabs.html.twig' %}
        {% block tabs %}
            {% embed 'shared/_tab.html.twig' with { route: 'app_micro_post_follows', classes: 'ml-2' } %}
                {% block content %}
                    From people you follow <span class="px-2 rounded-md bg-gray-200 dark:bg-gray-700" ></span>
                {% endblock %}
                {% block path %}
                    {{ path('app_micro_post_follows') }}
                {% endblock %}
            {% endembed %}
        {% endblock %}
    {% endembed %}
</div>